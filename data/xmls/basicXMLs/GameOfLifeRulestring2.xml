        <cell state="dead"/>
        <cell state="dead"/>
        <cell state="alive"/>
        <cell state="dead"/>
        <cell state="dead"/>

        <cell state="alive"/>
        <cell state="alive"/>
        <cell state="alive"/>
        <cell state="dead"/>
        <cell state="dead"/>

    </grid>

    <parameters>
        <parameter name="Rulestring" value="B2/S1"/>
    </parameters>
</simulation>